{{#with web}}
  <main>
    <h1>Languages:</h1>
    <ul>
      {{#each languages}}

        <li class='mt-5'>

          <h2>{{name}}</h2>
          <h3>Version: {{version}}</h3>
          <em>
            <p>{{description}}</p>
          </em>

          {{#if elements}}
            <hr class='my-5' />
            <h4>Elements:</h4>
            <ul>
              {{#each elements}}
                <li>
                  <strong>{{name}}</strong>
                  <p>{{description}}</p>

                  <h5>Attributes:</h5>
                  <ul>
                    {{#each attributes}}
                      <li>
                        <strong>{{name}}</strong>
                        <p>{{description}}</p>
                      </li>
                    {{/each}}
                  </ul>
                </li>
              {{/each}}
            </ul>
          {{else}}
            <hr class='my-5' />
            {{#if properties}}
              <h4>Properties:</h4>
              <ul>
                {{#each properties}}
                  <li>
                    <strong>{{name}}</strong>
                    <p>{{description}}</p>
                    <h4>Values:</h4>
                    <div class='mb-3'>
                      <ul>
                        {{#each values}}
                          <li>
                            <div>{{value}}</div>
                            <p>{{description}}</p>
                          </li>
                        {{/each}}
                      </ul>
                    </div>

                  </li>
                {{/each}}
              </ul>
            {{else}}
              <h4>Functions:</h4>
              <ul>
                {{#each functions}}
                  <li>
                    <strong>{{name}}</strong>
                    <p>{{description}}</p>
                    <div>
                      <h4>Parameters:</h4>
                      <ul>
                        {{#each parameters}}
                          <li>
                            <em>{{name}}</em>
                            <div>{{type}}</div>
                            <p>{{description}}</p>
                          </li>
                        {{/each}}
                      </ul>
                    </div>
                  </li>
                {{/each}}
              </ul>
            {{/if}}
          {{/if}}
        </li>
      {{/each}}
    </ul>
  </main>
{{/with}}